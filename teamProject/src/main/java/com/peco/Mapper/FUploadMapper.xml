<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.peco.Mapper.FUploadMapper">

	<insert id="insert">
		insert into F_img
		values(#{uuid}, #{uploadpath}, #{fileName}, #{fileType}, #{m_id})
	</insert>

	<delete id="delete">
		delete F_img
		where m_id=#{m_id}
		and uuid=#{uuid}
	</delete>
	
</mapper>
	